  "scripts": {
    "comments:win": "echo WINDOWS SPECIFIC RUN SCRIPTS",
    "start:win": "npm run build:win && npm run serve:win",
    "build:win": "npm run build:server:win && npm run build:client:win",
    "build:client:win": "SET platform=client&&webpack",
    "build:server:win": "SET platform=server&&webpack",
    "postbuild:win": "sh ./buildScript.sh",
    "serve:win": "SET APP_PORT=9000&& SET OPEN_WEATHER_MAP_GATEWAY=http://api.openweathermap.org/data/2.5/&& SET OPEN_WEATHER_MAP_API_KEY=3202bed939bd3cfc1a249ab5587566a0&& node ./dist/app.server.js",

